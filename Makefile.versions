S3_VERSION_MAJOR	:= 0
S3_VERSION_MINOR	:= 1
S3_VERSION_SUBLEVEL	:=
S3_VERSION_EXTRA	:=
S3_VERSION_NAME		:=
S3_VERSION		:= $(S3_VERSION_MAJOR)$(if $(S3_VERSION_MINOR),.$(S3_VERSION_MINOR))$(if $(S3_VERSION_SUBLEVEL),.$(S3_VERSION_SUBLEVEL))$(if $(S3_VERSION_EXTRA),.$(S3_VERSION_EXTRA))

export S3_VERSION_MAJOR S3_VERSION_MINOR S3_VERSION_SUBLEVEL
export S3_VERSION_EXTRA S3_VERSION_NAME S3_VERSION
